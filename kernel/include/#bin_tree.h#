#ifndef __bin_tree_
#define __bin_tree_

typedef struct tree tree;
typedef struct node node;

struct tree
{
    node *root;
    int size;
    int depth;
};

struct node
{
    node *left;
    node *right;
    void *address;
};

void init_tree(tree *t);
void  t, void *d);
void remove_node(tree *t, node *n);

#endif
